  field(INPB, "$(IOCNAME):$(QUEUE_TYPE)_Q_SIZE NPP MS")
  field(CALC, "100*A/B")
  field(LOPR, "0")
  field(HOPR, "100")
  field(EGU,  "%")
}

record(ai, "$(IOCNAME):$(QUEUE_CAPS)_Q_OVERRUNS") {
  field(DESC, "# of overruns of IOC's $(QUEUE) queue")
  field(SCAN, "I/O Intr")
  field(DTYP, "IOC stats")
  field(INP,  "@$(QUEUE)QueueOverruns")
  field(PINI, "YES")
}
